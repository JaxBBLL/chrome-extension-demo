{
  "name": "爱采购客保账号同步工具",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://b2bservice.baidu.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "notifications",
    "tabs",
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}
